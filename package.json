{
  "name": "mern-auth",
  "version": "1.0.0",
  "description": "Mern Auth Example",
  "main": "server.js",
  "engines": {
    "node": "12.13.1",
    "npm": "6.12.1"
  },
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js \"concurrently \" \"npm run server\" \"npm run client\"",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "install": "cd client && npm install"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ajax": "0.0.4",
    "axios": "^0.19.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.4.1",
    "cheerio": "^1.0.0-rc.3",
    "concurrently": "^4.0.1",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "file-system": "^2.2.2",
    "is-empty": "^1.2.0",
    "jquery": "^3.4.1",
    "jsonwebtoken": "^8.3.0",
    "matter-js": "^0.14.2",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "mongoose": "^5.3.11",
    "multer": "^1.4.2",
    "nodemon": "^2.0.2",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "path": "^0.12.7",
    "react-animated-css": "^1.2.1",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-moment": "^0.9.7",
    "schema-utils": "^2.6.4",
    "validator": "^10.9.0"
  }
}
